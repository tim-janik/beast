; BseProject

(bse-version "0.8.0")

(container-child "BseWaveRepo::Wave-Repository"
  (modification-time "2013-03-24 18:52:13")
  (creation-time "2006-12-23 14:38:01")
  (license "Creative Commons Attribution 2.5 (http://creativecommons.org/licenses/by/2.5/)")
  (author "Tim Janik"))
(container-child "BseCSynth::Sum-Diff-Test"
  (auto-activate #t)
  (modification-time "2013-03-24 18:52:13")
  (creation-time "2006-12-23 14:38:04")
  (license "Public Domain (https://creativecommons.org/publicdomain/zero/1.0/)")
  (author "Tim Janik")
  (blurb "The difference channel is used to invert the highpass")
  (container-child "BseSummation::Sum/Diff"
    (pos-y 1)
    (source-input "audio-in1" (link 1 "Modulated Pulse") "audio-out")
    (source-input "audio-in2" (link 1 "Highpass") "audio-out"))
  (container-child "BseStandardOsc::Modulated Pulse"
    (pulse-mod-perc 100)
    (pulse-width 50)
    (fm-n-octaves 1)
    (exponential-fm #f)
    (fm-perc 0)
    (base-freq 110)
    (wave-form bse-standard-osc-pulse)
    (pos-y 2)
    (pos-x -2)
    (source-input "pwm-in" (link 1 "Modulator") "audio-out"))
  (container-child "BseStandardOsc::Modulator"
    (pulse-mod-perc 0)
    (pulse-width 50)
    (fm-n-octaves 1)
    (exponential-fm #f)
    (fm-perc 0)
    (base-freq 1)
    (wave-form bse-standard-osc-triangle)
    (pos-y 1)
    (pos-x -4))
  (container-child "BseBiquadFilter::Highpass"
    (gain-perc 25)
    (fm-n-octaves 1)
    (exponential-fm #f)
    (fm-perc 25.382652878761292)
    (gain 9.0352945327758789)
    (freq 1774.6181640625)
    (filter-type bse-biquad-filter-resonant-highpass)
    (pos-x -2)
    (source-input "audio-in" (link 1 "Modulated Pulse") "audio-out")
    (source-input "freq-mod-in" (link 1 "Modulator") "audio-out"))
  (container-child "BsePcmOutput::PcmOutput-1"
    (pos-y 1)
    (pos-x 3)
    (source-input "left-audio-in" (link 1 "50%") "audio-out")
    (source-input "right-audio-in" (link 1 "50%") "audio-out"))
  (container-child "BseAmplifier::50%"
    (master-volume 1)
    (base-level 50)
    (ostrength 50)
    (ctrl-exp #f)
    (ctrl-mul #t)
    (clevel2 100)
    (clevel1 100)
    (alevel2 100)
    (alevel1 0)
    (pos-y 2)
    (pos-x 1)
    (source-input "audio-in2" (link 1 "Sum/Diff") "audio-diff")))
