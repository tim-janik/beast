                           0, 0, MAXINT, 1, STANDARD);
  Int         utime     = SfiInt (_("User Time"), _("The CPU time spent executing instructions of this thread"),
                           0, 0, MAXINT, 1, STANDARD);
  Int         stime     = SfiInt (_("System Time"), _("The CPU time spent in the system for this thread"),
                           0, 0, MAXINT, 1, STANDARD);
